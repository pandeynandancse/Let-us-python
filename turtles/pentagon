import turtle
s = turtle.Screen()
a=turtle.Turtle()
a.pensize(5)
a.shape('turtle')
a.speed(1)
a.right(72)
a.forward(100)
a.stamp()
a.color('blue')
a.right(72)
a.forward(100)
a.stamp()
a.color('yellow')
a.right(72)
a.forward(100)
a.stamp()
a.color('red')
a.right(72)
a.forward(100)
a.stamp()
a.color('green')
a.right(72)
a.forward(100)
